<table class="table tabel-borderd table-hover table-striped table-info">
    <thead>
        <tr>
            <th>S.no</th>
            <th>Fullname</th>
            <th>Email</th>
            <th>Contact</th>
            <th>EDIT/DELETE</th>

        </tr>
    </thead>
    <tbody>
        <%for(let i=0; i < user.length; i++){%>
            <tr>
                <td><%= i+1%></td>
                <td><%= user[i].firstname+""+user[i].lastname;%></td>
                <td><%= user[i].email;%></td>
                <td><%= user[i].contact;%></td>
                <td>
                    <a href="/edituser?editid=<%= user[i].id;%>" class="btn btn-primary">Edit</a>
                    <a href="/deleteuser?deleteid=<%= user[i].id%>" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        <%}%>
    </tbody>
</table>